    {
      "parameters": {
        "jsCode": "// Lấy dữ liệu HTML từ node HTTP Request2\nconst html = $node[\"HTTP Request\"].json[\"data\"] || \"\";\n\n// Ghi log HTML (giới hạn 1000 ký tự)\nconsole.log(\"Product Code:\", $node[\"Google Sheets\"].json[\"Mô tả sản phẩm\"] || \"Unknown\");\nconsole.log(\"HTML:\", html.substring(0, 1000));\n\n// Danh sách các regex để thử\nconst regexes = [\n  // Định dạng: (W) 36mm x (H) 81mm x (D) 66.5mm\n  /\\(W\\)\\s*(\\d+\\.?\\d*mm)\\s*x\\s*\\(H\\)\\s*(\\d+\\.?\\d*mm)\\s*x\\s*\\(D\\)\\s*(\\d+\\.?\\d*mm)/,\n  // Định dạng: 36mm x 81mm x 66.5mm\n  /(\\d+\\.?\\d*mm)\\s*x\\s*(\\d+\\.?\\d*mm)\\s*x\\s*(\\d+\\.?\\d*mm)/,\n  // Định dạng: Width: 18mm, Height: 90mm, Depth: 70mm\n  /Width:\\s*(\\d+\\.?\\d*mm)\\s*,?\\s*Height:\\s*(\\d+\\.?\\d*mm)\\s*,?\\s*Depth:\\s*(\\d+\\.?\\d*mm)/i,\n  // Định dạng: 36 x 85 x 68 mm\n  /(\\d+\\.?\\d*\\s*x\\s*\\d+\\.?\\d*\\s*x\\s*\\d+\\.?\\d*\\s*mm)/,\n  // Định dạng: W:36mm H:81mm D:66.5mm\n  /W:(\\d+\\.?\\d*mm)\\s*H:(\\d+\\.?\\d*mm)\\s*D:(\\d+\\.?\\d*mm)/i\n];\n\n// Thử từng regex\nlet dimensions = \"Không tìm thấy\";\nfor (const regex of regexes) {\n  const match = html.match(regex);\n  if (match) {\n    dimensions = match[1] + \" x \" + match[2] + \" x \" + match[3];\n    break;\n  }\n}\n\n// Ghi log kết quả\nconsole.log(\"Dimensions:\", dimensions);\n\n// Trả về kết quả\nreturn [{\n  json: {\n    product_code: $node[\"Google Sheets\"].json[\"Mô tả sản phẩm\"] || \"Unknown\",\n    manufacturer: $node[\"Google Sheets\"].json[\"Hãng\"] || \"Unknown\",\n    dimensions: dimensions\n  }\n}];"
